@model De2_AI_Agent.Models.StudentAccomodationSuper

@{
    ViewBag.Title = "ShowRecommendations";
}

<h2>ShowRecommendations</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Name
        </th>
        <th>
            Location
        </th>
        <th>
            Decsription
        </th>
        <th>
            ImageUrl
        </th>
        <th>
            Incomegroup
        </th>
        <th>
            AccomodationOwnerUserId
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.overalRecommendations)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImageUrl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IncomeGroup)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccomodationOwnerUsersId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>

